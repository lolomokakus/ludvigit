#!/usr/bin/env python3
import sys

def greg(string):
    words = string.split()
    output = ""
    for word in words:
        testword = word.lower()
        if testword == "i":
            output += "Greg "
        elif testword == "me":
            output += "Greg "
        elif testword == "my":
            output += "Greg's "
        elif testword == "mine":
            output += "Greg's "
        elif testword == "myself":
            output += "himself "
        elif testword == "am":
            output += "is "
        elif testword == "i'm":
            output += "Greg is "
        elif testword == "have":
            output += "has "
        elif testword == "i've":
            output += "Greg has "
        else:
            output += word + " "
    print(output.rstrip())

def main():
    if len(sys.argv) > 1:
        greg(" ".join(sys.argv[1:]))
    else:
        for line in sys.stdin:
            greg(line)

try:
    main()
except KeyboardInterrupt:
    sys.exit(1)
