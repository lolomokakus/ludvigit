#!/usr/bin/env python3
import sys # haha

def sus(string):
    words = string.split()
    output = ""
    for word in words:
        if len(word) % 2 == 0:
            output += word[:len(word) // 2] + word[len(word) // 2 - 1::-1] + " "
        else:
            output += word[:len(word) // 2 + 1] + word[len(word) // 2 - 1::-1] + " "
    print(output.rstrip())

if len(sys.argv) > 1:
    sus(" ".join(sys.argv[1:]))
else:
    for line in sys.stdin:
        sus(line)
