print_info() {
	info title
	info underline
	info "OS" distro
	info "Kernel" kernel
	info "Packages" packages
	info "Uptime" uptime
	info "Shell" shell
	info "CPU" cpu
	info "GPU" gpu
	info "Memory" memory
	info "Storage" disk
}

